<h1>Listing job_change_orders</h1>

<table>
  <thead>
    <tr>
      <th>Job site</th>
      <th>Co number</th>
      <th>Co total</th>
      <th>Notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @job_change_orders.each do |job_change_order| %>
      <tr>
        <td><%= job_change_order.job_site_id %></td>
        <td><%= job_change_order.co_number %></td>
        <td><%= job_change_order.co_total %></td>
        <td><%= job_change_order.notes %></td>
        <td><%= link_to 'Show', job_change_order %></td>
        <td><%= link_to 'Edit', edit_job_change_order_path(job_change_order) %></td>
        <td><%= link_to 'Destroy', job_change_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

